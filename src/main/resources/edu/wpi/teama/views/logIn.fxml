<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teama.controllers.LogInController">
   <children>
      <Button fx:id="exitButton" layoutX="514.0" layoutY="32.0" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
      <TextField fx:id="usernameBox" layoutX="225.0" layoutY="187.0" promptText="Username" />
      <TextField fx:id="passwordBox" layoutX="225.0" layoutY="239.0" promptText="Password" />
      <Text layoutX="194.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome!">
         <font>
            <Font name="Comic Sans MS Bold" size="47.0" />
         </font>
      </Text>
      <Button fx:id="logInButton" layoutX="274.0" layoutY="287.0" mnemonicParsing="false" onAction="#logIn" text="Log In" />
   </children>
</AnchorPane>
